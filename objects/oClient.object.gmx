            <string>/// keep send something

// wait for other
scr_wait_other();

if (room == atk02) {
    // send key to server
    scr_send_key_to_socket(server);
    
    // send pos to server
    scr_send_pos();
    
    // when you win
    if (room != r0 &amp;&amp; oEX.hp &lt;= 0) {
        scr_send_hp(-1,server);
    }

    ///keep alive (ping)
    if (!isping &amp;&amp; !sendping &amp;&amp; letsping) {
        scr_ping();
        sendping = true;
        temptime = current_time;
        pingtimes += 1; 
        letsping = false;
    }
    
    if (isping) {
        ping = round((current_time - temptime)/room_speed);
        isping = false;
        sendping = false;
        alarm[0] = room_speed;
    }
}
